{
  "name": "monk-components",
  "version": "1.0.1",
  "main": "dist/monk-components.umd.js",
  "module": "dist/monk-components.esm.js",
  "types": "dist/index.d.ts",
  "private": false,
  "type": "module",
  "license": "MIT",
  "author": "zhanxin",
  "keywords": [
    "Component",
    "UI",
    "Vue3"
  ],
  "homepage": "https://github.com/luozhanxin/monk-components.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/luozhanxin/monk-components.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:esm && npm run build:umd",
    "build:esm": "rollup --config build/rollup.esm.config.js",
    "build:umd": "rollup --config build/rollup.umd.config.js",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "test:watch": "vitest --environment jsdom --root tests/",
    "test": "vitest run --environment jsdom --root tests/",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.3",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^4.0.4",
    "rollup": "^3.10.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.12"
  }
}